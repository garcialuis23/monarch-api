{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Run Spring Boot App",
			"type": "shell",
			"command": "java",
			"args": [
				"-cp",
				"target/classes;${env:HOME}/.m2/repository/org/springframework/boot/spring-boot-starter-web/3.2.5/*",
				"com.stem.MonarcaAPl.MonarcaAPlApplication"
			],
			"group": "build",
			"isBackground": true,
			"problemMatcher": [
				"$tsc"
			]
		}
	]
}